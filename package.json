{
  "name": "revSyntax",
  "displayName": "RevSyntax",
  "description": "Language support for RevBayes",
  "version": "0.1.2",
  "publisher": "Benjamin-So",
  "engines": {
    "vscode": "^1.93.0"
  },
  "main": "./src/extension.js",
  "activationEvents": [
    "onLanguage:rev"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "keybindings": [
      {
        "key": "Ctrl+Enter",
        "mac": "Cmd+Eneter",
        "command": "runRevCommands",
        "when": "editorLangId == 'rev'"
      }
    ],
    "languages": [
      {
        "id": "rev",
        "aliases": [
          "RevBayes",
          "rev"
        ],
        "extensions": [
          ".rev",
          ".Rev"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rev",
        "scopeName": "source.rev",
        "path": "./syntaxes/rev.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BenjaminSoYH/revSyntax.git"
  }
}
